{
  "name": "faux-hollows",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run",
    "test:ci:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:ui:debug": "playwright test --ui --debug",
    "lint": "eslint .",
    "script:load-community-data": "node --max-old-space-size=8192 --loader ts-node/esm --es-module-specifier-resolution=node ./scripts/load-community-data.ts --path"
  },
  "dependencies": {
    "@floating-ui/vue": "^0.1.0",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@playwright/experimental-ct-vue": "^1.33.0",
    "@playwright/test": "^1.33.0",
    "@types/glob": "^8.1.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.15.0",
    "@types/prettier": "^2.7.2",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-vue": "^9.13.0",
    "glob": "^8.1.0",
    "playwright": "^1.33.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.7",
    "vite-plugin-checker": "^0.6.0",
    "vitest": "^0.31.0",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "1.7.3",
    "yargs": "^17.7.1"
  }
}
