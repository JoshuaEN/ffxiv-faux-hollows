  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vue": "vue",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "pnpm run build && pnpm run host-dist-preview",
    "host-dist-preview": "pnpm wr pages dev",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run",
    "test:ci:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:ui:debug": "playwright test --ui --debug",
    "lint": "eslint .",
    "typecheck": "pnpm vue-tsc --noEmit",
    "wr": "pnpm wrangler",
    "script:load-community-data": "pnpm tsx ./scripts/load-community-data/load-community-data.ts --path",
    "script:create-recursive-fast-lookups": "node ./scripts/recursive-fast-lookups/build-recursive-fast-lookups.js",
    "postscript:create-recursive-fast-lookups": "node ./dist-scripts/recursive-fast-lookups/create-recursive-fast-lookups.js",
    "script:full-solve": "node ./scripts/solvers/full-solve/build-full-solve.js",
    "postscript:full-solve": "node ./dist-scripts/solvers/full-solve/full-solve.js",
    "script:full-solve:all:debug-last-attempt": "node ./dist-scripts/solvers/full-solve/full-solve.js",
    "script:full-solve:all": "node ./scripts/solvers/full-solve/run-all-solvers.js",
    "script:full-solve-e2e": "pnpm tsx ./scripts/solvers/full-solve-e2e/full-solve.ts",
    "script:short-circuit:full-solve": "node ./scripts/solvers/short-circuit-solve/build-full-solve.js",
    "postscript:short-circuit:full-solve": "node ./dist-scripts/solvers/short-circuit-solve/full-solve.js",
    "script:short-circuit:full-solve:all:debug-last-attempt": "node ./dist-scripts/solvers/short-circuit-solve/full-solve.js",
    "script:short-circuit:full-solve:all": "node ./scripts/solvers/short-circuit-solve/run-all-solvers.js"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.50.0",
    "@types/glob": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.10",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/eslint-config-typescript": "^14.3.0",
    "concurrently": "^9.1.2",
    "decimal.js": "^10.5.0",
    "easy-table": "^1.2.0",
    "esbuild": "^0.24.2",
    "eslint": "9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "glob": "^11.0.1",
    "playwright": "^1.50.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "sharp": "^0.33.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^2.1.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0",
    "wrangler": "^3.105.0",
    "yargs": "^17.7.2"
  },
  "packageManager": "pnpm@9.0.6+sha1.648f6014eb363abb36618f2ba59282a9eeb3e879",
