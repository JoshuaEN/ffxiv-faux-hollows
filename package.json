{
  "name": "faux-hollows",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vue": "vue",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run",
    "test:ci:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:ui:debug": "playwright test --ui --debug",
    "lint": "eslint .",
    "script:load-community-data": "node --max-old-space-size=8192 --loader ts-node/esm --es-module-specifier-resolution=node ./scripts/load-community-data.ts --path",
    "script:sim": "pnpx esbuild --define:import.meta.env.DEV=true --platform=node ./scripts/sim/sim.ts --sourcemap --source-root=./ --bundle --format=esm --outfile=./dist/sim/sim.js && node ./dist/sim/sim.js",
    "script:full-solve": "node ./scripts/full-solve/build-full-solve.js",
    "postscript:full-solve": "node --max-old-space-size=8152 ./dist/full-solve/full-solve.js",
    "script:full-solve:all:debug-last-attempt": "node ./dist/full-solve/full-solve.js",
    "script:full-solve:all": "node ./scripts/full-solve/run-all-solvers.js"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.6",
    "vue": "^3.4.26"
  },
  "devDependencies": {
    "@playwright/experimental-ct-vue": "^1.43.1",
    "@playwright/test": "^1.43.1",
    "@types/glob": "^8.1.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.8",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.5.3",
    "decimal.js": "^10.4.3",
    "esbuild": "^0.21.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.25.0",
    "glob": "^10.3.12",
    "playwright": "^1.43.1",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^1.5.3",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^1.8.27",
    "yargs": "^17.7.2"
  }
}
